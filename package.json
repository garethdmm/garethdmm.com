{
  "name": "garethdmm-dot-com",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "build:docker": "source .env.local && docker build -t gdm-com --build-arg DATABASE_URL=$LOCAL_DOCKER_DATABASE_URL . ",
    "start:docker": "docker run -d -p 3000:3000 --name gdm-com gdm-com",
    "stop:docker": "docker stop gdm-com && docker rm gdm-com",
    "lint": "next lint",
    "db:migrate:gen": "./scripts/db-migrate-gen.sh",
    "db:migrate:diff": "./scripts/db-migrate-diff.sh",
    "db:migrate:up": "./scripts/db-migrate-up.sh",
    "db:reset": "source .env.local && DATABASE_URL=$DATABASE_URL prisma migrate reset",
    "db:seed": "source .env.local && psql $DATABASE_URL -f seed.sql"
  },
  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.0",
    "@headlessui/react": "^2.1.1",
    "@heroicons/react": "^2.1.1",
    "@mdxeditor/editor": "^3.8.0",
    "@mui/icons-material": "^5.15.12",
    "@mui/lab": "^5.0.0-alpha.167",
    "@mui/material": "^5.15.12",
    "@next/third-parties": "^14.1.4",
    "@prisma/client": "^5.16.1",
    "clsx": "^2.1.1",
    "next": "13.4.8",
    "next-mdx-remote": "^5.0.0",
    "posthog-js": "^1.139.3",
    "react": "^18",
    "react-dom": "^18",
    "react-icons": "^5.0.1",
    "react-vertical-timeline-component": "^3.6.0",
    "reading-time": "^1.5.0"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.13",
    "@types/node": "20.14.9",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/react-vertical-timeline-component": "^3.3.6",
    "autoprefixer": "^10.0.1",
    "eslint": "^8",
    "eslint-config-next": "14.1.3",
    "postcss": "^8",
    "tailwindcss": "^3.3.0",
    "typescript": "5.5.3"
  }
}
